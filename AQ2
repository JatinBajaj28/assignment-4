#include <bits/stdc++.h>
using namespace std;

void sortQueue(queue<int>& q) {
    int n = q.size();
    for(int i = 0; i < n; i++) {
        int maxVal = INT_MIN;
        int size = q.size();
        for(int j = 0; j < size; j++) {
            int curr = q.front(); q.pop();
            if(curr > maxVal && j < size - i) maxVal = curr;
            q.push(curr);
        }
        int count = 0;
        size = q.size();
        for(int j = 0; j < size; j++) {
            int curr = q.front(); q.pop();
            if(curr == maxVal && count == 0) { count++; continue; }
            q.push(curr);
        }
        q.push(maxVal);
    }
}

int main() {
    queue<int> q;
    vector<int> arr = {11,5,4,21};
    for(int x : arr) q.push(x);

    sortQueue(q);

    while(!q.empty()) {
        cout << q.front() << " ";
        q.pop();
    }
}
